                    <input type="date" id="FechaNacAfiliadoFamiliar" name="FechaNacAfiliadoFamiliar" class="input100" onchange="changeCampos()">

                </div>
                <div class="w3-third ">
                    <button class="w3-right w3-margin-top enviar w3-card bg-zul w3-text-white w3-hover-green w3-hover-border-cyan" style="margin: 0 0 0 200px;" type="submit" onclick="insertarFamiliar()">
                        Agregar Familiar
                    </button>
                </div>
                <div class="w3-third">
                </div>
            </div>

            <div class="w3-row">
                <div class="container">
                    <table>
                        <thead>
                            <tr>
                                <th>Familiar</th>
                                <th>Nombre</th>
