                    <input type="date" id="FechaIngresoAfiliadoEmpresa" name="FechaIngresoAfiliadoEmpresa" class="input100" onchange="changeCampos()">

                </div>
                <div class="w3-third w3-container wrap-input100 bg1 rs1-wrap-input100">
                    <span class="label-input100">Nombre Empresa</span>
                    <input type="text" id="NombreEmpresaAfiliadoEmpresa" name="NombreEmpresaAfiliadoEmpresa" class="input100" onchange="changeCampos()">
                </div>
                <div class="w3-third w3-container wrap-input100 bg1 rs1-wrap-input100">
